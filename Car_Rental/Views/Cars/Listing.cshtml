@model IEnumerable<Car>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width-device-width, initial-scale=1.0" />
    <!--Bootstrap-->
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
    <!-- Font Awesome CSS -->
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css'>
    <!-- jQuery -->
    <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0"
            crossorigin="anonymous"></script>
    <!--Local CSS-->
    <link rel="stylesheet" href="\css\Cars.css">
    <title>Cars</title>

</head>

<body class="d-flex flex-column min-vh-100">
    <!--About Cars-->
    <div class="jumbotron text-center">
        <div class="container-fluid">
            <h1 style="color: rgb(0, 0, 0);text-shadow: 2px 2px 4px #374644;">Luxury category</h1>
            <p>
                <span style="font-weight: bold;">Style, spaces, confort?</span> Experience the joy of driving premium
                cars.
            </p><br>

            <!--Search field-->
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md"></div>
                    <div class="col-md-4">
                        <form class="d-flex" style="display: flex; justify-content: end;">
                            <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                            <button class="btn btn-outline-primary" type="submit">Search</button>
                        </form>
                    </div>
                    <div class="col-md"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col" style="text-align:end">
            <a href='@Url.Action("Create","Cars")' class="btn btn-outline-dark">
                New Car
            </a>
        </div>
    </div><br />
        <!--List of vechicle types-->
        <div class="container-fluid">
            <div class="row">
                @for (int i = 0; i < Model.Count(); i++)
                {
                    if (i > 0 && i % 4 == 0)
                    {
                    @:</div><br /><br /><div class="row">
                        // close and start new row
                    }

                    <!--Card Code-->
                    <div class="col-md d-flex align-items-stretch">
                        <div class="card" style="width: 18rem;">
                            <img class="card-img-top"
                                 src=@Html.DisplayFor(modelItem => Model.ElementAt(i).PictureURL)
                                 alt="Card image cap">
                            <div class="card-body">
                                <p class="card-title" href="#"
                                   style="color: rgb(68, 86, 94);font-weight: bold;font-size: 14pt;">
                                    <span>@Html.DisplayFor(modelItem => Model.ElementAt(i).Model)</span>
                                </p>
                                <br>
                                <div class="card-text">
                                    <div class="container-fluid">
                                        <div class="row">
                                            <div class="col-md">
                                                <img src="https://sx-content-labs.sixt.io/img/svg/Icons-black/24x24/ico_people.svg"
                                                     alt="">
                                                <span>7</span>
                                            </div>
                                            <div class="col-md">
                                                <img src="https://sx-content-labs.sixt.io/img/svg/Icons-black/24x24/ico_door.svg"
                                                     alt="">
                                                <span>4</span>
                                            </div>
                                            <div class="col-md-6">
                                                <img src="https://sx-content-labs.sixt.io/img/svg/Icons-black/24x24/ico_gearbox.svg"
                                                     alt="">
                                                <span>@Html.DisplayFor(modelItem => Model.ElementAt(i).TransmisionType)</span>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="row">
                                            <div class="col-md">
                                                <img src="https://sx-content-labs.sixt.io/img/svg/Icons-black/40x40/ico_fuelbig.svg"
                                                     alt="">
                                                <span>@Html.DisplayFor(modelItem => Model.ElementAt(i).FuelType)</span>
                                            </div>
                                            <div class="col-md" style="padding-top: 3%;">
                                                <img src="https://sx-content-labs.sixt.io/img/svg/Icons-black/24x24/ico_snowflake.svg"
                                                     alt="">
                                                @if (Model.ElementAt(i).ClimateControll)
                                                {
                                                    <span>AC</span>
                                                }
                                            </div>
                                        </div>
                                    </div>

                                    <!--Price-->
                                    <section class="pricing py-5">
                                        <h6 class="card-price text-center">$0<span class="period">/day</span></h6>
                                    </section>
                                </div>
                            </div>

                            <!--Rating-->
                            <div id="car-container">
                                <!--car rating-->
                            </div>

                            <div class="card-footer">
                                <div class="row">
                                    <div class="col-6">
                                        <button type="button" class="btn btn-primary btn-block" data-bs-toggle="modal"
                                                data-bs-target="#detailsModal">
                                            Details
                                        </button>
                                    </div>
                                    <div class="col-6">
                                        <button type="button" class="btn btn-success btn-block" data-bs-toggle="modal"
                                                data-bs-target="#reservationModal">
                                            Book now
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                }


